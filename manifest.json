{
    "manifest_version": 2,
    "name": "Plurk-MyEmoCache",
    "version": "1.1",
    "description": "Plurk 表情符號快取，避免每次瀏覽都重新下載。",
    "icons": {
        "128": "128.png",
        "16": "16.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://*.plurk.com/*",
        "https://*.plurk.com/*"
    ]
}